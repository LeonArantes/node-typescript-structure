  "version": "1.0.0",
  "main": "index.js",
  "author": "Leon Arantes",
  "license": "MIT",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
